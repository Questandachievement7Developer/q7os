<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema
        id="com.github.wwmm.pulseeffects.sinkinputs.convolver"
        path="/com/github/wwmm/pulseeffects/sinkinputs/convolver/">
        <key name="state" type="b">
            <default>false</default>
        </key>
        <key name="installed" type="b">
            <default>true</default>
        </key>
        <key name="post-messages" type="b">
            <default>true</default>
        </key>
        <key name="input-gain" type="d">
            <range min="-20.0" max="20.0"/>
            <default>0.0</default>
        </key>
        <key name="output-gain" type="d">
            <range min="-20.0" max="20.0"/>
            <default>0.0</default>
        </key>
        <key name="kernel-path" type="s">
            <default>""</default>
        </key>
        <key name="ir-width" type="i">
            <range min="0" max="200"/>
            <default>100</default>
        </key>
    </schema>
</schemalist>