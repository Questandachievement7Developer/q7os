<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema
        id="com.github.wwmm.pulseeffects.sourceoutputs.gate"
        path="/com/github/wwmm/pulseeffects/sourceoutputs/gate/">
        <key name="state" type="b">
            <default>false</default>
        </key>
        <key name="installed" type="b">
            <default>true</default>
        </key>
        <key name="post-messages" type="b">
            <default>true</default>
        </key>
        <key name="detection" enum="com.github.wwmm.pulseeffects.gate.detection.enum">
            <default>"RMS"</default>
        </key>
        <key
