<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema
        id="com.github.wwmm.pulseeffects.sinkinputs.maximizer"
        path="/com/github/wwmm/pulseeffects/sinkinputs/maximizer/">
        <key name="state" type="b">
            <default>false</default>
        </key>
        <key name="installed" type="b">
            <default>true</default>
        </key>
        <key name="post-messages" type="b">
            <default>true</default>
        </key>
        <key name="release" type="d">
            <range min="1" max="100"/>
            <default>3.16</default>
        </key>
        <key name="ceiling" type="d">
            <range min="-30" max="0"/>
            <default>0.0</default>
        </key>
        <key name="threshold" type="d">
            <range min="-30" max="0"/>
            <default>0.0</default>
        </key>
    </schema>
</schemalist>