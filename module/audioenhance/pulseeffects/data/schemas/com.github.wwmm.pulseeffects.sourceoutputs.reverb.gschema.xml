<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema
        id="com.github.wwmm.pulseeffects.sourceoutputs.reverb"
        path="/com/github/wwmm/pulseeffects/sourceoutputs/reverb/">
        <key name="state" type="b">
            <default>false</default>
        </key>
        <key name="installed" type="b">
            <default>true</default>
        </key>
        <key name="post-messages" type="b">
            <default>true</default>
        </key>
        <key name="input-gain" type="d">
            <range min="-36.1" max="36.1"/>
            <default>0.0</default>
        </key>
        <key name="output-gain" type="d">
            <range min="-36.1" max="36.1"/>
            <default>0.0</default>
        </key>
        <key name="room-size" enum="com.github.wwmm.pulseeffects.reverb.roomsize.enum">
            <default>"Large"</default>
        </key>
        <key name="decay-time" type="d">
            <range min="0.4" max="15"/>
            <default>1.5</default>
        </key>
        <key name="hf-damp" type="d">
            <range min="2000" max="20000"/>
            <default>5000</default>
        </key>
        <key name="diffusion" type="d">
            <range min="0" max="1"/>
            <default>0.5</default>
        </key>
        <key name="amount" type="d">
            <range min="-100" max="6"/>
            <default>-12</default>
        </key>
        <key name="dry" type="d">
            <range min="-100" max="6"/>
            <default>0</default>
        </key>
        <key name="predelay" type="d">
            <range min="0" max="500"/>
            <default>0</default>
        </key>
        <key name="bass-cut" type="d">
            <range min="20" max="20000"/>
            <default>300</default>
        </key>
        <key name="treble-cut" type="d">
            <range min="20" max="20000"/>
            <default>5000</default>
        </key>
    </schema>
</schemalist>