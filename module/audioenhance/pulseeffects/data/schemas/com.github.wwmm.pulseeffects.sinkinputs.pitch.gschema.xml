<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema
        id="com.github.wwmm.pulseeffects.sinkinputs.pitch"
        path="/com/github/wwmm/pulseeffects/sinkinputs/pitch/">
        <key name="state" type="b">
            <default>false</default>
        </key>
        <key name="installed" type="b">
            <default>true</default>
        </key>
        <key name="post-messages" type="b">
            <default>true</default>
        </key>
        <key name="cents" type="d">
            <range min="-100" max="100"/>
            <default>0</default>
        </key>
        <key name="semitones" type="i">
            <range min="-12" max="12"/>
            <default>0</default>
        </key>
        <key name="octaves" type="i">
            <range min="-3" max="3"/>
            <default>0</default>
        </key>
        <key name="crispness" type="i">
            <range min="0" max="3"/>
            <default>3</default>
        </key>
        <key name="formant-preserving" type="b">
            <default>false</default>
        </key>
        <key name="faster" type="b">
            <default>false</default>
        </key>
        <key name="input-gain" type="d">
            <range min="-20.0" max="20.0"/>
            <default>0.0</default>
        </key>
        <key name="output-gain" type="d">
            <range min="-20.0" max="20.0"/>
            <default>0.0</default>
        </key>
    </schema>
</schemalist>